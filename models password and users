password.dart
class Password {
  final int? id;
  final int userId;
  final String title;
  final String username;
  final String password;

  Password({
    this.id,
    required this.userId,
    required this.title,
    required this.username,
    required this.password,
  });

  // Konversi dari Map ke Object
  factory Password.fromMap(Map<String, dynamic> map) {
    return Password(
      id: map['id'],
      userId: map['userId'],
      title: map['title'],
      username: map['username'],
      password: map['password'],
    );
  }
}


user.dart
class User {
  int? id;
  String username;
  String fullName; // Tambahan kolom full_name
  String password;

  User({
    this.id,
    required this.username,
    required this.fullName,
    required this.password,
  });

  Map<String, dynamic> toMap() {
    return {
      'id': id,
      'username': username,
      'full_name': fullName, 
      'password': password,
    };
  }

  factory User.fromMap(Map<String, dynamic> map) {
    return User(
      id: map['id'],
      username: map['username'],
      fullName: map['full_name'], 
      password: map['password'],
    );
  }
}
